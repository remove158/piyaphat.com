{
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "@/components/*": ["./src/components/*"],
            "@/layouts/*": ["./src/layouts/*"]
        },
        "strictNullChecks": true,
        "allowJs": true
    },
    "extends": "astro/tsconfigs/strict"
}
